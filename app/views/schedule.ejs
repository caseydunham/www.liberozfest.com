      <div class="event-speakers">
        <div class="row justify-content-center speaker">
          <div class="col-lg-12">
            <div class="title">Breakfast</div>
            <div class="presenter-name">KTO</div>
            <div class="time"><i class="fa fa-clock-o">&nbsp;9:00am - 10:00am</i> <i class="fa fa-map-marker">&nbsp;Nymph Garden</i></div>
          </div>
        </div>
